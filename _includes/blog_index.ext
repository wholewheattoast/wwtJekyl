<!-- Show last 5 posts here -->
{% for post in paginator.posts %}
  {% include post.ext %}
{% endfor %}

  <nav id="blog-pagination">
    <ul class="list-unstyled list-inline">
      {% if paginator.next_page %}
        <li>
          <a href="{{site.baseurl}}{{paginator.next_page_path}}" rel="next">older</a>
        </li>
      {% endif %}
      <li><a href="{{site.baseurl}}/blog/archive.html">archive</a></li>
      {% if paginator.previous_page %}
        <li>
          <a href="{{site.baseurl}}{{paginator.previous_page_path}}" rel="prev">newer</a>
        </li>
      {% endif %}
    </ul>
  </nav>

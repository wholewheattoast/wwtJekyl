---
layout: base_full_width
title: {{sb_display_name}} {{spread}}
description: {{sb_display_name}} {{spread}}
stub: sketchbooks/{{sb_url_safe_name}}/index.html
date: 
---

<article itemscope itemtype="http://schema.org/VisualArtwork" class="sketchbook_page">
  <div class="row">
    <div class="col-md-10">
      <img 
        itemprop="image"
        src="/image/sketchbooks/{{sb_url_safe_name}}/{{sb_url_safe_name}}-{{spread}}.jpg" 
        title="{{sb_display_name}} {{spread}}" 
        alt="" 
      />
    </div>
  </div>
  <div class="row">
    <div class="col-md-12">
      <h2 itemprop="name">{{spread}}</h2>
      <nav>
        <ul class="list-unstyled sb-nav">
          <li>
            <a href="{{sb_url_safe_name}}-{{prev}}.html" rel="prev" title="prev">prev | {{prev}}</a>
          </li>
          <li>
            <a href="index.html" title="index">index</a>
          </li>
          <li>
            <a href="{{sb_url_safe_name}}-{{next}}.html" rel="next" title="next">next | {{next}}</a>
          </li>
        </ul>
      </nav>
    </div>
  </div>
  <div class="row">
  <!-- once i get partials to work, replace -->
    {{#verso_metadata}}
      <section class="col-md-6 col-sm-6" itemscope itemtype="http://schema.org/VisualArtwork">
        <h3 itemprop="name">{{sb_display_name}} | {{page_number}}</h3>
        <dl class="inline_dl">
          {{#created}}<dt>Date:</dt><dd><time itemprop="dateCreated" datetime="{{created}}">{{created}}</time></dd>{{/created}}
          {{#earliest_date}}<dt>Earliest Date:</dt><dd><time itemprop="dateCreated" datetime="{{earliest_date}}">{{earliest_date}}</time></dd>{{/earliest_date}}
          {{#latest_date}}<dt>Latest Date:</dt><dd><time itemprop="dateCreated" datetime="{{latest_date}}">{{latest_date}}</time></dd>{{/latest_date}}
          {{#contributor}}<dt>contributor:</dt><dd><span itemprop="contributor">{{contributor}}</span></dd>{{/contributor}}
          {{#location}}<dt>Location:</dt><dd><span itemprop="locationCreated">{{location}}</span></dd>{{/location}}
          {{#medium}}<dt>Medium:</dt><dd><span itemprop="artform">{{medium}}</span></dd>{{/medium}}
          {{#description}}<dt>Description:</dt><dd itemprop="description">{{description}}</dd></dd>{{/description}}
          {{#subject}}<dt>Subject:</dt><dd><span itemprop="about">{{subject}}</span></dd>{{/subject}}
          {{#transcription}}<dt>Transcription:</dt><dd><span itemprop="text">{{transcription}}</span></dd>{{/transcription}}
        </dl>
      </section>
      {{> sb_page_metadata}}
    {{/verso_metadata}}

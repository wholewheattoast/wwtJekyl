---
layout: post
title: {{{title}}}
tags: {{{formated_tags}}}
date: {{display_date}}
---

{{#images}}

  <p>
    <img
      srcset="
        {{#low_resolution}}
        /image/posts/{{local}} {{width}}w,
        {{/low_resolution}}
        {{#standard_resolution}}
        /image/posts/{{local}} {{width}}w,
        {{/standard_resolution}}
        {{#full_resolution}}
        /image/posts/{{local}} 1080w
        {{/full_resolution}}
        "
      src="/image/posts/{{#standard_resolution}}{{local}}{{/standard_resolution}}"
      alt=""
      title="{{title}}"
    />
  </p>

{{/images}}


{{#caption}}
{{#cleaned_text}}
<p>{{{cleaned_text}}}</p>
{{/cleaned_text}}
{{/caption}}

{{#location}}
<div class="post_location" itemscope itemtype="http://schema.org/Place">
  <p>
    At <span itemprop="name">{{name}}</span>
    <div itemprop="geo" itemscope itemtype="http://schema.org/GeoCoordinates">
      <meta itemprop="latitude" content="{{latitude}}" />
      <meta itemprop="longitude" content="{{longitude}}" />
    </div>
  </p>
</div>
{{/location}}

